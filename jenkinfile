pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Coding') {
            steps {
                echo 'Coding'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('RElease') {
            steps {
                echo 'Releasinging'
            }
        }
    }
}
